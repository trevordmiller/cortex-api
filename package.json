{
  "name": "cortex-api",
  "version": "0.1.0",
  "description": "The API for Cortex",
  "private": true,
  "repository": "trevordmiller/cortex-api",
  "license": "MIT",
  "author": "Trevor D. Miller (https://trevordmiller.com)",
  "main": "build/index.js",
  "scripts": {
    "dev:build": "yarn build -- --watch",
    "dev:server": "nodemon build/index.js",
    "verify": "yarn typecheck && yarn build",
    "typecheck": "flow",
    "build": "babel src -d build",
    "release": "git checkout master && git pull && now",
    "start": "node build/index.js"
  },
  "babel": {
    "presets": [
      "env",
      "flow"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.44.2",
    "nodemon": "^1.11.0",
    "now": "^4.11.1"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "express-graphql": "^0.6.4",
    "graphql": "^0.9.3",
    "micro": "^7.3.2",
    "micro-cors": "^0.0.4",
    "mongodb": "^2.2.25"
  }
}
